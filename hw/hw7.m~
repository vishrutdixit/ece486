close all
s = tf('s');

as = [.01 0.01 1 10 100];

Mps = zeros(1,size(as,2));

idx = 0;
for a = as
    sys = ((s/a) + 1) /(s^2 + s + 1);
    idx = idx+1;
    t = 0:.01:12;
    %figure(idx), step(sys,t)
    [Mp, tr, ts] = StepResponseMetrics(step(sys,t), t, 1, 1);
     Mp
     Mr = getPeakGain(sys)
     bode(sys)
end

